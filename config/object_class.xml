    </objectClass>

    <objectClass>
        <name> function </name>

        <source> 
            {db}.sys.objects AS {alias} WHERE [type] IN (N'FN', N'FT', N'IF')
        </source>
        <source_alias> F </source_alias>

        <metadata_keys>
            <metadata_key>
                <name> schema_id </name>
                <type> INT </type>
            </metadata_key>
            <metadata_key>
                <name> object_id </name>
                <type> INT </type>
            </metadata_key>
        </metadata_keys>
        
        <object_keys>
            <object_key>
                <name> schema_name </name>
                <type> SYSNAME </type>
            </object_key>
            <object_key>
                <name> function_name </name>
                <type> SYSNAME </type>
            </object_key>
        </object_keys>
        
        <view_schema_name> sys </view_schema_name>
        <view_name> objects </view_name>
    </objectClass>

    <objectClass>
        <name> function_param </name>

        <source> 
             {db}.sys.parameters AS {alias} WHERE EXISTS
             (
                SELECT * 
                FROM {db}.sys.objects 
                WHERE {db}.sys.objects.[object_id] = {alias}.[object_id] 
                        AND 
                        {db}.sys.objects.[type] IN (N'FN', N'FT', N'IF')
             )
        </source>
        <source_alias> P </source_alias>

        <metadata_keys>
            <metadata_key>
                <name> object_id </name>
                <type> INT </type>
            </metadata_key>
            <metadata_key>
                <name> parameter_id </name>
                <type> INT </type>
            </metadata_key>
        </metadata_keys>
        
        <object_keys>
            <object_key>
                <name> function_name </name>
                <type> SYSNAME </type>
            </object_key>
            <object_key>
                <name> parameter_name </name>
                <type> SYSNAME </type>
            </object_key>
        </object_keys>
        
        <view_schema_name> sys </view_schema_name>
        <view_name> parameters </view_name>
    </objectClass>

</objectClasses>