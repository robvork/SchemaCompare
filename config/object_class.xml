    <objectClass>
        <name> function </name>

        <source> 
            {db}.sys.objects AS {alias} 
            WHERE [type] IN (N'FN', N'FT', N'IF')
        </source>
        <source_alias> F </source_alias>

        <metadata_keys>
            <metadata_key>
                <name> object_id </name>
                <type> INT </type>
