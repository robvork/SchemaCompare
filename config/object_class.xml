    <objectClass>
        <name> procedure_param </name>

        <source> 
            {db}.sys.parameters AS {alias} WHERE EXISTS
            (
                SELECT * 
                FROM {db}.sys.procedures 
                WHERE {db}.sys.procedures.[object_id] = {alias}.[object_id]
            )
        </source>
        <source_alias> P </source_alias>

        <metadata_keys>
            <metadata_key>
                <name> object_id </name>
                <type> INT </type>
            </metadata_key>
            <metadata_key>
                <name> parameter_id </name>
                <type> INT </type>
            </metadata_key>
        </metadata_keys>
        
        <object_keys>
            <object_key>
                <name> procedure_name </name>
                <type> SYSNAME </type>
            </object_key>
            <object_key>
                <name> parameter_name </name>
                <type> SYSNAME </type>
            </object_key>
        </object_keys>
        
        <view_schema_name> sys </view_schema_name>
