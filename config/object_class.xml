<objectClasses>
    <objectClass>
        <name> database </name>

        <source>
            {db}.sys.databases AS {alias} 
        </source>
        <source_alias> D </source_alias>

        <metadata_keys>
            <metadata_key>
                <name> database_id </name>
                <type> INT </type>
            </metadata_key>
        </metadata_keys>
        
        <object_keys>
            <object_key>
                <name> name </name>
                <type> SYSNAME </type>
            </object_key>
        </object_keys>
        
        <view_schema_name> sys </view_schema_name>
        <view_name> databases </view_name>
    </objectClass>

    <objectClass>
        <name> schema </name>

        <source>
            {db}.sys.schemas AS {alias}
        </source>
        <source_alias> S </source_alias>
        
        <metadata_keys>
            <metadata_key>
                <name> schema_id </name>
                <type> INT </type>
            </metadata_key>
        </metadata_keys>
        
        <object_keys>
            <object_key>
                <name> schema_name </name>
                <type> SYSNAME </type>
            </object_key>
        </object_keys>
        
        <view_schema_name> sys </view_schema_name>
        <view_name> schemas </view_name>
    </objectClass>

    <objectClass>
        <name> table </name>

        <source> 
            {db}.sys.tables AS {alias}
        </source>
        <source_alias> T </source_alias>

        <metadata_keys>
            <metadata_key>
                <name> schema_id </name>
                <type> INT </type>
            </metadata_key>
            <metadata_key>
                <name> object_id </name>
                <type> INT </type>
            </metadata_key>
        </metadata_keys>
        
        <object_keys>
            <object_key>
                <name> schema_name </name>
                <type> SYSNAME </type>
            </object_key>
            <object_key>
                <name> table_name </name>
                <type> SYSNAME </type>
            </object_key>
        </object_keys>
        
        <view_schema_name> sys </view_schema_name>
        <view_name> tables </view_name>
    </objectClass>

    <objectClass>
        <name> table_column </name>

        <source> 
            {db}.sys.columns AS {alias} WHERE EXISTS 
             (
                SELECT * 
                FROM {db}.sys.tables 
                WHERE {db}.sys.tables.[object_id] = {alias}.[object_id]
             )
            
        </source>
        <source_alias> C </source_alias>

        <metadata_keys>
            <metadata_key>
                <name> object_id </name>
                <type> INT </type>
            </metadata_key>
            <metadata_key>
                <name> column_id </name>
                <type> INT </type>
            </metadata_key>
        </metadata_keys>
        
        <object_keys>
            <object_key>
                <name> table_name </name>
                <type> SYSNAME </type>
            </object_key>
            <object_key>
                <name> column_name </name>
                <type> SYSNAME </type>
            </object_key>
        </object_keys>
        
        <view_schema_name> sys </view_schema_name>
        <view_name> columns </view_name>
    </objectClass>

