<objectClasses>
    <objectClass>
        <name> database </name>

        <source>
            {db}.sys.databases AS {alias}
            WHERE {alias}.[name] = '{db}' 
        </source>
        <source_alias> D </source_alias>

        <metadata_keys>
            <metadata_key>
                <name> source_database_id </name>
                <type> INT </type>
                <column> {alias}.[database_id] </column>
            </metadata_key>
        </metadata_keys>
        
        <object_keys>
            <object_key>
                <name> source_database_name </name>
                <type> SYSNAME </type>
                <column> {alias}.[name] </column>
            </object_key>
        </object_keys>
        
        <view_schema_name> sys </view_schema_name>
        <view_name> databases </view_name>
    </objectClass>

